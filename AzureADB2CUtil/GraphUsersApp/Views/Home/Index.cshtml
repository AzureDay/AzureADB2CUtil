@model IEnumerable<UserViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<table>
    <thead class="table">
    <tr>
        <th scope="col">User Id</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
        </tr>
    }
    </tbody>
</table>
